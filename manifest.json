{
    "manifest_version": 3,
    "name": "Mi Primera Extensión",
    "version": "1.0",
    "description": "Una extensión simple para empezar",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "assets/icon.png"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    }
}